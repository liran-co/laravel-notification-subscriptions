{
    "name": "liran-co/laravel-notification-subscriptions",
    "description": "Notification subscription management.",
    "homepage": "https://github.com/liran-co/laravel-notification-subscriptions",
    "keywords": [
        "laravel",
        "subscription",
        "subscriptions",
        "notification",
        "notifications"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Liran Cohen",
            "email": "l@liran.co"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0|^8.1",
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "LiranCo\\NotificationSubscriptions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LiranCo\\NotificationSubscriptions\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LiranCo\\NotificationSubscriptions\\NotificationSubscriptionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
